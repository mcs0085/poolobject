  <testsuite name="ubu.gii.dass.test.c01.DuplicatedInstanceExceptionTest" time="0.0">
    <testcase name="testDuplicatedInstanceException" classname="ubu.gii.dass.test.c01.DuplicatedInstanceExceptionTest" time="0.0"/>
  </testsuite>
</testrun>
